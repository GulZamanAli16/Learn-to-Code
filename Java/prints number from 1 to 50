public class lab3_task1 {
    public static void main(String[] args) {
        
          System.out.println("we are printing numbers ");
          
        System.out.println("Print numbers from 1 to 50 :");
        
        // using for loop
        for(int i=1;i<=50;i++){
            System.out.print(i +" , ");
        }
    }
}
